    <class>org.labican.sabia.modelo.Distrito</class>
    <class>org.labican.sabia.modelo.Categoria</class>
    <class>org.labican.sabia.modelo.Localidade</class>
    <class>org.labican.sabia.modelo.Usuario</class>
    <class>org.labican.sabia.modelo.TipoStatusAvaliador</class>
    <class>org.labican.sabia.modelo.RG</class>
